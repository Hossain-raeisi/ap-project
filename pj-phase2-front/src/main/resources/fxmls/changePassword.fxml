<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="1080.0" prefWidth="1920.0" style="-fx-background-color: #bcbcbc;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="front.app.fxml_controllers.ChangePassword">
    <Pane layoutX="760.0" layoutY="349.0" prefHeight="382.0" prefWidth="400.0" style="-fx-background-color: #424242;">
        <TextField fx:id="password1Field" layoutX="80.0" layoutY="88.0" prefHeight="50.0" prefWidth="240.0" promptText="new password">
            <font>
                <Font size="18.0" />
            </font>
        </TextField>
        <TextField fx:id="password2Field" layoutX="80.0" layoutY="148.0" prefHeight="50.0" prefWidth="240.0" promptText="confirm new password">
            <font>
                <Font size="18.0" />
            </font>
        </TextField>
        <Button fx:id="changePasswordButton" layoutX="131.0" layoutY="280.0" mnemonicParsing="false" onAction="#changePasswordButtonAction" prefHeight="40.0" prefWidth="138.0" style="-fx-background-color: #3299bb;" text="change password">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
      <TextField fx:id="oldPasswordField" layoutX="80.0" layoutY="208.0" prefHeight="50.0" prefWidth="240.0" promptText="old password">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
    </Pane>
</AnchorPane>
